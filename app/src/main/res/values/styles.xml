<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/primaryLightColor</item>

    </style>

    <style name="MiBotonRaised" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/secondaryColor</item>
        <item name="colorControlHighlight">@color/secondaryLightColor</item>
        <item name="android:textColor">@color/primaryTextColor</item>
    </style>

    <style name="MiBotonRaised2" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/primaryColor</item>
        <item name="colorControlHighlight">@color/secondaryLightColor</item>
        <item name="android:textColor">@color/primaryTextColor</item>
    </style>
</resources>